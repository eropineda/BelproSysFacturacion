@model BelproSysFacturacion.Models.CountryModels

@{
    ViewData["Title"] = "Delete";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h3>¿Desea Eliminar el registro?</h3>
<div>
   
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.CountryName)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.CountryName)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UserEnrollmentInsert)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserEnrollmentInsert)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EnrollmentDateInsert)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnrollmentDateInsert)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.UserEnrollmentModify)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.UserEnrollmentModify)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.EnrollmentDateModify)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EnrollmentDateModify)
        </dd>
    </dl>
    
    <form asp-action="DeleteCountry">
        <input type="hidden" asp-for="CountryISO" />
        <input type="submit" value="Eliminar" class="btn btn-danger" /> 

        <a class="btn btn-secondary " asp-action="Index" asp-controller="Country">Cancelar</a>
    </form>
</div>
